        {
          "_customTemplate": true,
          "_afterNavbarItem": false,
          "_customHTML": "\n<link href=\"https://www.jqueryscript.net/css/jquerysctipttop.css\" rel=\"stylesheet\" type=\"text/css\">\n<link href=\"https://www.jqueryscript.net/demo/Form-Submit-Buttons-with-Built-in-Loading-Indicators-Ladda/css/demo.css\" rel=\"stylesheet\" type=\"text/css\">\n<link href=\"https://www.jqueryscript.net/demo/Form-Submit-Buttons-with-Built-in-Loading-Indicators-Ladda/dist/ladda.min.css\" rel=\"stylesheet\" type=\"text/css\">\n[JS_CODE_0]\n\n<style>\ninput[type=email], select {\n width: 100%;\n padding: 12px 20px;\n margin: 8px 0;\n display: inline-block;\n border: 1px solid #ccc;\n border-radius: 4px;\n box-sizing: border-box;\n background-color: #fffff;\n}\n\ninput[type=submit] {\n width: 100%;\n background-color: green;\n color: white;\n padding: 14px 20px;\n margin: 8px 0;\n border: none;\n border-radius: 4px;\n cursor: pointer;\n}\ninput[type=text], select {\n width: 100%;\n padding: 12px 20px;\n margin: 8px 0;\n display: inline-block;\n border: 1px solid #ccc;\n border-radius: 4px;\n box-sizing: border-box;\n background-color: #fffff;\n}\ninput[type=number], select {\n width: 100%;\n padding: 12px 20px;\n margin: 8px 0;\n display: inline-block;\n border: 1px solid #ccc;\n border-radius: 4px;\n box-sizing: border-box;\n background-color: #fffff;\n}\ninput[type=tel], select {\n width: 100%;\n padding: 12px 20px;\n margin: 8px 0;\n display: inline-block;\n border: 1px solid #ccc;\n border-radius: 4px;\n box-sizing: border-box;\n background-color: #fffff;\n}\ninput[type=submit]:hover {\n background-color: brown;\n}\n\ninput[type=email] {\n border-radius: 5px;\npadding: 20px;\n background-color: #fffff;\n}\n</style>\n\n[JS_CODE_1]\n\n<form id=\"form\" target=\"_self\" onsubmit=\"return postToGoogle();\" action=\"\" autocomplete=\"off\"> \n<input id=\"nameField\" name=\"202769964\" placeholder=\"Full name\" type=\"text\" required=\"\"> \n<input id=\"genderField\" name=\"202769964\" placeholder=\"Gender\" type=\"text\" required=\"\">\n<input id=\"marryField\" name=\"202769964\" placeholder=\"Marital Status\" type=\"text\" required=\"\"> \n<input id=\"numberField\" name=\"202769964\" placeholder=\"Phone Number\" type=\"tel\" required=\"\"> \n<input id=\"emailField\" name=\"202769964\" placeholder=\"Email\" type=\"email\" required=\"\">\n<input id=\"stateField\" name=\"202769964\" placeholder=\"State\" type=\"text\" required=\"\">\n\n<input id=\"lgaField\" name=\"202769964\" placeholder=\"LGA\" type=\"text\" required=\"\">\n<input id=\"addField\" name=\"202769964\" placeholder=\"Address\" type=\"text\" required=\"\">\n\n<section class=\"button-demo\">\n<button class=\"ladda-button\" data-color=\"green\" data-style=\"contract\">Submit</button>\n</section>\n\n[JS_CODE_1] \n[JS_CODE_2]\n</form>\n\n\n\n\n[JS_CODE_3]",
