            "<script src=\"https://www.jqueryscript.net/demo/Form-Submit-Buttons-with-Built-in-Loading-Indicators-Ladda/dist/ladda.min.js\"></script>",
            "<script>\nLadda.bind( '.button-demo button', { timeout: 7000 } );\n\nfunction postToGoogle() {\n var field0 = $(\"#nameField\").val();\n var field1 = $(\"#genderField\").val();\n var field2 = $(\"#marryField\").val();\n var field3 = $(\"#numberField\").val();\n var field4 = $(\"#emailField\").val();\n var field5 = $(\"#stateField\").val();\n var field6 = $(\"#lgaField\").val();\n var field7 = $(\"#addField\").val();\n\n \n \n if(field0 == \"\"){\n alert('Please enter Your Email');\n document.getElementById(\"nameField\").focus();\n return false;\n }\n if(field1 == \"\"){\n alert('Please enter Your Name');\n document.getElementById(\"genderField\").focus();\n return false;\n }\n if(field2 == \"\"){\n alert('Marital Status');\n document.getElementById(\"dobField\").focus();\n return false;\n }\n if(field3 == \"\"){\n alert('Please enter Whatsapp Number');\n document.getElementById(\"numberField\").focus();\n return false;\n }\n if(field4 == \"\"){\n alert('Please enter Whatsapp Number');\n document.getElementById(\"emailField\").focus();\n return false;\n }\n if(field5 == \"\"){\n alert('Please enter Whatsapp Number');\n document.getElementById(\"lgaField\").focus();\n return false;\n }\n if(field6 == \"\"){\n alert('Please enter Whatsapp Number');\n document.getElementById(\"wardField\").focus();\n return false;\n }\n if(field7 == \"\"){\n alert('Please enter Whatsapp Number');\n document.getElementById(\"polField\").focus();\n return false;\n }\n \n \n $.ajax({\n url: \"https://docs.google.com/forms/d/e/1FAIpQLSemMW1VqaIm_ec8nKMOvsf6eSg8A1l7KjCccjFva2pFJsbJdg/formResponse?\",\n data: {\"entry.1873598520\": field0,\"entry.120982572\": field1,\"entry.1397843010\": field2,\"entry.900202295\": field3,\"entry.1043534050\": field4,\"entry.921726352\": field5,\"entry.1418703009\": field6,\"entry.261107344\": field7},\n type: \"POST\",\n dataType: \"xml\",\n success: function(d)\n {\n },\n error: function(x, y, z)\n {\n \n $('#form').hide();\n alert(\"Registered Successfully! Please Make payment to complete your registration\");\nwindow.location.href =\"#payment\";\n\n\n \n }\n });\n return false;\n }\n \n</script>"
